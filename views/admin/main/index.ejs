<% include ../playout/header.ejs %>
    <!-- END TOPBAR -->
    <!-- START CONTAINER -->
    <div class="page-container row-fluid container-fluid">
        <!-- SIDEBAR - START -->

        <% include ../playout/menu.ejs %>


            <!--  SIDEBAR - END -->
            <!-- START CONTENT -->
            <section id="main-content" class=" ">
                <section class="wrapper main-wrapper row" style=''>
                    <div class='col-xs-12'>
                        <div class="page-title">
                            <div class="pull-left">
                                <h1 class="title">Dashboard</h1>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>

                    <div class="clearfix"></div>
                    <div class="col-lg-12">
                        <section class="box nobox marginBottom0">
                            <div class="content-body">
                                <div class="row">
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="r4_counter db_box">
                                            <i class='pull-left fa fa-user icon-md icon-rounded icon-primary'></i>
                                            <div class="stats">
                                                <h4><strong>
                                                        <%-account%>
                                                    </strong></h4>
                                                <span>Account</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="r4_counter db_box">
                                            <i class='pull-left fa fa-users icon-md icon-rounded icon-accent'
                                                style="padding-right: 30px;padding-left:17px"></i>
                                            <div class="stats">
                                                <h4><strong>
                                                        <%-user%>
                                                    </strong></h4>
                                                <span>User</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="r4_counter db_box">
                                            <i class='pull-left fa fa-newspaper icon-md icon-rounded icon-warning'></i>
                                            <div class="stats">
                                                <h4><strong>
                                                        <%-post%>
                                                    </strong></h4>
                                                <span>Post</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- End .row -->
                            </div>
                        </section>
                    </div>
                </section>
            </section>
            <!-- END CONTENT -->
